{
  "manifest_version": 2,
  "name": "Simple Search Tab G",
  "short_name": "Simple Search Tab G",
  "version": "2.0.0",
  "description": "Simple search form to query Google when you open a new tab.",
  "homepage_url": "https://github.com/garysweaver/simple_search_tab_g",
  "chrome_url_overrides": {"newtab": "simple_search_tab_g.html"},
  "minimum_chrome_version": "22",
  "browser_specific_settings": {
    "gecko": {
      "id": "simple_search_tab_g.garysweaver@example.com",
      "strict_min_version": "54.0"
    }
  }
}
